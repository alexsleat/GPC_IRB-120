MODULE MainModule
	VAR string str_item := "alex"
	PROC main()
		IF str_item = "hello" THEN
			 hello ;
		ENDIF
		ELSEIF str_item = "this" THEN
			 this string ;
		ENDIF
		ELSEIF str_item = "that" THEN
			 that thing ;
		ENDIF
		ELSEIF str_item = "alex" THEN
			 alex is awesome ;
		ENDIF
		ELSEIF str_item = "the" THEN
			 the film im watching is already terrible ;
		ENDIF
		ELSEIF str_item = "testy" THEN
			 testy mctesterson the 3rd ;
		ENDIF
	ENDPROC
ENDMODULE
